{
  "name": "blog-project",
  "version": "5.0.0",
  "scripts": {
    "generate:client-env-vars": "node generate-client-env-vars.js",
    "generate:server-env-vars": "node generate-server-env-vars.js",
    "dev": "npm run build && npm start",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "npm run generate:client-env-vars && ng build --prod",
    "build:server": "npm run generate:server-env-vars && ng run blog-project:server:production",
    "start": "node dist/server/main.js",
    "test": "ng lint",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "ng lint",
    "lint:styles": "stylelint src/**/*.scss",
    "db-migration:rename-quote-to-epigraph": "ts-node -O '{\"module\": \"commonjs\"}' server/dal/migrations/2-rename-quote-to-epigraph"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.0",
    "@angular/common": "10.2.0",
    "@angular/compiler": "10.2.0",
    "@angular/core": "10.2.0",
    "@angular/forms": "10.2.0",
    "@angular/language-service": "10.2.0",
    "@angular/platform-browser": "10.2.0",
    "@angular/platform-browser-dynamic": "10.2.0",
    "@angular/platform-server": "10.2.0",
    "@angular/router": "10.2.0",
    "@nguniversal/express-engine": "10.1.0",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "@sentry/browser": "5.27.1",
    "@sentry/node": "5.27.1",
    "axios": "0.21.0",
    "body-parser": "1.19.0",
    "codelyzer": "6.0.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "core-js": "3.6.5",
    "cors": "2.8.5",
    "envkey": "1.2.8",
    "express": "4.17.1",
    "express-jwt": "5.3.3",
    "express-robots": "0.1.6",
    "intersection-observer": "0.11.0",
    "jsonwebtoken": "8.5.1",
    "moment": "2.29.1",
    "ng-autosize": "1.1.0",
    "ng-in-viewport": "6.1.4",
    "rxjs": "6.6.3",
    "sitemap": "4.1.1",
    "slug": "1.1.0",
    "tslib": "2.0.3",
    "zone.js": "~0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "10.2.0",
    "@angular/compiler-cli": "10.2.0",
    "@types/body-parser": "1.19.0",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.8",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "12.12.68",
    "codelyzer": "6.0.1",
    "rimraf": "3.0.2",
    "stylelint": "10.1.0",
    "stylelint-config-recommended-scss": "3.3.0",
    "stylelint-scss": "3.10.0",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  },
  "heroku-run-build-script": true
}
