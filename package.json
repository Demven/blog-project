{
  "name": "blog-project",
  "version": "6.0.1",
  "scripts": {
    "generate:client-env-vars": "node generate-client-env-vars.js",
    "generate:server-env-vars": "node generate-server-env-vars.js",
    "dev": "npm run build && npm start",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "npm run generate:client-env-vars && ng build --prod",
    "build:server": "npm run generate:server-env-vars && ng run blog-project:server:production",
    "start": "node dist/server/main.js",
    "test": "ng lint",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "ng lint",
    "lint:styles": "stylelint src/**/*.scss",
    "db-migration:rename-quote-to-epigraph": "ts-node -O '{\"module\": \"commonjs\"}' server/dal/migrations/2-rename-quote-to-epigraph"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.14",
    "@angular/common": "11.2.14",
    "@angular/compiler": "11.2.14",
    "@angular/core": "11.2.14",
    "@angular/forms": "11.2.14",
    "@angular/language-service": "11.2.14",
    "@angular/platform-browser": "11.2.14",
    "@angular/platform-browser-dynamic": "11.2.14",
    "@angular/platform-server": "11.2.14",
    "@angular/router": "11.2.14",
    "@nguniversal/express-engine": "11.2.1",
    "@sentry/browser": "6.4.1",
    "@sentry/node": "6.4.1",
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "codelyzer": "^6.0.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "core-js": "3.12.1",
    "cors": "2.8.5",
    "envkey": "1.3.0",
    "express": "4.17.1",
    "express-jwt": "5.3.3",
    "express-robots": "0.1.6",
    "intersection-observer": "0.12.0",
    "jsonwebtoken": "8.5.1",
    "moment": "2.29.1",
    "ng-autosize": "1.1.0",
    "ng-in-viewport": "6.1.5",
    "rxjs": "6.6.7",
    "sitemap": "4.1.1",
    "slug": "5.0.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.13",
    "@angular/cli": "11.2.13",
    "@angular/compiler-cli": "11.2.14",
    "@types/body-parser": "1.19.0",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.11",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "8.5.1",
    "@types/node": "14.17.0",
    "codelyzer": "^6.0.1",
    "rimraf": "3.0.2",
    "stylelint": "10.1.0",
    "stylelint-config-recommended-scss": "3.3.0",
    "stylelint-scss": "3.10.0",
    "tslint": "6.1.3",
    "typescript": "4.1.5"
  },
  "heroku-run-build-script": true,
  "resolutions": {
    "webpack": "^5.0.0"
  }
}
