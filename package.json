{
  "name": "dmitry-salnikov-blog-project",
  "version": "1.12.0",
  "description": "Personal blog",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node dist/server/app.js",
    "dev": "npm run build && npm start",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:server && npm run build:client && npm run build:images && npm run build:assets-scripts",
    "build:client": "webpack --progress --profile --bail",
    "build:server": "tsc --p tsconfig-server.json",
    "build:images": "cpy public/images/* dist/client/public/images",
    "build:assets-scripts": "cpy public/scripts/* dist/client/public/scripts",
    "db:reset": "npm run build && node dist/server/dal/seed",
    "lint:styles": "stylelint src/**/*.pcss",
    "lint:ts": "tslint -c tslint.json src/**/*.ts"
  },
  "license": "UNLICENSED",
