    "@angular/forms": "7.2.1",
    "@angular/http": "7.2.1",
    "@angular/language-service": "7.2.1",
    "@angular/platform-browser": "7.2.1",
    "@angular/platform-browser-dynamic": "7.2.1",
    "@angular/platform-server": "7.2.1",
    "@angular/router": "7.2.1",
    "@nguniversal/express-engine": "7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "7.0.2",
    "@sentry/browser": "4.5.2",
    "@sentry/node": "4.5.2",
    "axios": "0.18.0",
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "codelyzer": "4.5.0",
    "compression": "1.7.3",
    "cookie-parser": "1.4.3",
    "core-js": "2.6.2",
    "envkey": "1.2.4",
    "express": "4.16.4",
    "express-jwt": "5.3.1",
    "express-robots": "0.1.6",
    "intersection-observer": "0.5.1",
    "jsonwebtoken": "8.4.0",
