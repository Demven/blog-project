{
  "name": "blog-project",
  "version": "3.13.1",
  "scripts": {
    "generate:env-vars": "node generate-env-vars.js",
    "dev": "npm run build && npm start",
    "clean": "rimraf dist",
    "build": "npm run generate:env-vars && npm run clean && npm run build:client && npm run build:ssr && npm run build:server",
    "build:client": "ng build --configuration production",
    "build:ssr": "ng run blog-project:server",
    "build:server": "webpack --config webpack.server.config.js --progress --colors",
    "start": "node dist/server/server.js",
    "test": "ng lint",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "ng lint",
    "lint:styles": "stylelint src/**/*.scss",
    "db-migration:add-keywords": "ts-node -O '{\"module\": \"commonjs\"}' server/dal/seed/1-add-keywords"
  },
  "private": true,
