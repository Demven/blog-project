    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "core-js": "3.2.1",
    "envkey": "1.2.7",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "express-robots": "0.1.6",
    "intersection-observer": "0.7.0",
