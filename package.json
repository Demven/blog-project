{
  "name": "blog-project",
  "version": "4.1.0",
  "scripts": {
    "generate:env-vars": "node generate-env-vars.js",
    "dev": "npm run build && npm start",
    "clean": "rimraf dist",
    "build": "npm run generate:env-vars && npm run clean && npm run build:client && npm run build:ssr && npm run build:server",
    "build:client": "ng build --configuration production",
    "build:ssr": "ng run blog-project:server",
    "build:server": "webpack --config webpack.server.config.js --progress --colors",
    "start": "node dist/server/server.js",
    "test": "ng lint",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "ng lint",
    "lint:styles": "stylelint src/**/*.scss",
    "db-migration:rename-quote-to-epigraph": "ts-node -O '{\"module\": \"commonjs\"}' server/dal/migrations/2-rename-quote-to-epigraph"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.6",
    "@angular/common": "10.1.6",
    "@angular/compiler": "10.1.6",
    "@angular/core": "10.1.6",
    "@angular/forms": "10.1.6",
    "@angular/language-service": "10.1.6",
    "@angular/platform-browser": "10.1.6",
    "@angular/platform-browser-dynamic": "10.1.6",
    "@angular/platform-server": "10.1.6",
    "@angular/router": "10.1.6",
    "@nguniversal/express-engine": "10.1.0",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "@sentry/browser": "5.26.0",
    "@sentry/node": "5.26.0",
    "axios": "0.20.0",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "codelyzer": "^5.1.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "core-js": "3.6.5",
    "cors": "2.8.5",
    "envkey": "1.2.8",
    "express": "4.17.1",
    "express-jwt": "5.3.3",
    "express-robots": "0.1.6",
    "intersection-observer": "0.11.0",
    "jsonwebtoken": "8.5.1",
    "moment": "2.29.1",
    "mongoose": "5.10.9",
    "mongoose-unique-validator": "2.0.3",
    "ng-autosize": "1.1.0",
    "ng-in-viewport": "6.0.3",
    "rxjs": "6.6.3",
    "sitemap": "4.1.1",
    "slug": "1.1.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.7",
    "@angular/cli": "10.1.6",
    "@angular/compiler-cli": "10.1.6",
    "@types/bluebird": "3.5.32",
    "@types/body-parser": "1.19.0",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.8",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mongoose": "5.7.36",
    "@types/node": "12.12.68",
    "codelyzer": "5.2.2",
    "rimraf": "3.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-recommended-scss": "3.3.0",
    "stylelint-scss": "3.10.0",
    "ts-loader": "6.0.4",
